                        <li><a asp-controller="Partners" asp-action="PartnerBio" asp-route-id="Thomas A Lee III"><i class="fa fa-caret-right"></i> Thomas A Lee</a></li>
                        <li><a asp-controller="Partners" asp-action="PartnerBio" asp-route-id="Sandra K Curtin"><i class="fa fa-caret-right"></i> Sandra K Curtin</a></li>
                        <li><a asp-controller="Partners" asp-action="PartnerBio" asp-route-id="William J Becket"><i class="fa fa-caret-right"></i> William J Becket</a></li>
                        <li><a asp-controller="Partners" asp-action="PartnerBio" asp-route-id="John D Sheehan"><i class="fa fa-caret-right"></i> John D Sheehan</a></li>
                        <li><a asp-controller="Partners" asp-action="PartnerBio" asp-route-id="Gilbert B Weisman"><i class="fa fa-caret-right"></i> Gilbert B Weisman</a></li>
                    </ul>
                </li>
                <li><a asp-controller="App" asp-action="OurServices">Services</a></li>
                <li><a asp-controller="Events" asp-action="News">News</a></li>
                <li><a asp-controller="Events" asp-action="Events">Events</a></li>
                <li><a asp-controller="Events" asp-action="Pubs">Publications</a></li>
                <li><a asp-controller="App" asp-action="Index">Cases of Interest</a></li>
                <li><a asp-controller="App" asp-action="FirmHistory">Firm History</a></li>
                <li><a asp-controller="App" asp-action="Contact">Contact</a></li>
                @if (User.Identity.IsAuthenticated)
                {
                    <li class="divider"></li>
                    <li><a asp-controller="Auth" asp-action="Index">Manage Website</a></li>
                    <li><a asp-controller="Auth" asp-action="Logout">Logout</a></li>
                }
            </ul>
        </nav>
        <div class="affiliation">
            <table>
                <tr>
                    <td>
                        <img src="~/image/WBE_color_rgb_UP25.jpg" alt="" class="affiliationImage" />
                    </td>
                </tr>
            </table>
        </div>

        <div>
            <ul class="rightAlign">
                <li>&nbsp;</li>
                <li>Becket & Lee LLP</li>
                <li>16 General Warren Blvd</li>
                <li>Malvern, PA 19355</li>
                <li>&nbsp;</li>
                <li>610.644.7800</li>

            </ul>
        </div>

    </div>

    <div id="wrapper">
        <div id="main" class="container-fluid">
            @RenderBody()
        </div>
    </div>



    <div id="footer" class="container-fluid">
        <div class="navbar navbar-fixed-bottom navbar-inverse ">
            <ul>
                <li class="navbar-link">&copy; Becket & Lee LLP</li>
                <li class="navbar-link text-muted"><a id="disclaimerLink" href="#">Disclaimer</a></li>
            </ul>
        </div>
