@model string
@using BecketLee.Models

<form asp-action="DeletePartnerBio" role="form">
    @Html.Partial( "_ModalHeader", new ModalHeader { Heading = "Delete Partner Biography" } )

    <div class="modal-body form-horizontal">
        Are you sure you want to delete the biography for @Model?
    </div>
    @Html.Partial( "_ModalFooter", new ModalFooter { SubmitButtonText = "Delete" } )
</form>