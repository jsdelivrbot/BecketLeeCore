@{
    ViewBag.Title = "Upload File";    
}
<div class="container">
    <div class="row">
        <div class="absoluteCenter isResponsive">
            
            <form action="" method="post" enctype="multipart/form-data">
                <div class="col-md-5">
                    <div class="form-group text-center">
                        <label for="fileUpload">Photo Upload</label>
                        <input for="fileUpload" type="file" />
                    </div>
                    <div class="form-group text-center">
                        <input type="submit" value="Save Bio" />
                    </div>
                </div>
            </form>

        </div>

    </div>
</div>

